                    android:clipToPadding="false"
                    android:gravity="center_horizontal">

                    <com.google.android.material.imageview.ShapeableImageView
                        android:id="@+id/rn2mf63cl0hf"
                        android:layout_width="117dp"
                        android:layout_height="118dp"
