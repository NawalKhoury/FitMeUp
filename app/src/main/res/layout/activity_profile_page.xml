                <LinearLayout
                    android:id="@+id/r9f0qxa3gnms"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="30dp"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/rwqg4jf4gjm9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Dima Haj"
                        android:textColor="#000000"
                        android:textSize="32sp" />

                    <LinearLayout
                        android:id="@+id/r5zjunrh2rkx"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="4dp"
                        android:gravity="center_vertical"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/r7dlrsdxqojv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="9dp"
                            android:text="Age : "
                            android:textColor="#000000"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/rd24t617aakm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="4dp"
                            android:text="24"
                            android:textColor="#000000"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/r8vzedx0f8ho"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:text="yrs"
                            android:textColor="#000000"
                            android:textSize="13sp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <!-- WEIGHT AND HEIGHT SECTION -->
            <LinearLayout
                android:id="@+id/riweph8u1koh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="51dp"
                android:layout_marginBottom="19dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <!-- Weight Section -->
                <LinearLayout
                    android:id="@+id/r2hjh837w2d6"
                    android:layout_width="136dp"
                    android:layout_height="match_parent"
                    android:background="@drawable/cr30beff1f9"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical"
                    android:paddingHorizontal="39dp"
                    android:paddingTop="19dp"
                    android:paddingBottom="33dp">

                    <TextView
                        android:id="@+id/re52vi3n7lgt"
                        android:layout_width="69dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
